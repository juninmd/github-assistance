# Example .env file for local development
# NEVER commit this file to Git!

# Required: GitHub Personal Access Token
# Get it from: https://github.com/settings/tokens
# Required scopes: repo, workflow
GITHUB_TOKEN=ghp_your_token_here

# Required: Jules API Key
# Get it from Jules dashboard
JULES_API_KEY=AQ.Ab8RN6I2K0-WGNo9DnoRQOV2mmq_6Cv-4XpMpPpOjXUOdWADuQ

# Required for PR Assistant: Gemini API Key
# Get it from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_key_here

# Optional: GitHub owner/organization to monitor
GITHUB_OWNER=juninmd

# Optional: Telegram notifications
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

# Optional: Agent toggles (true/false)
PM_AGENT_ENABLED=true
UI_AGENT_ENABLED=true
DEV_AGENT_ENABLED=true
PR_ASSISTANT_ENABLED=true

# Optional: Repository allowlist path
REPOSITORY_ALLOWLIST_PATH=config/repositories.json

# Optional: Debug mode
DEBUG=false
